{
    "name": "dustin/exception",
    "type": "library",
    "description": "A small exception library to handle error codes in Symfony applications",
    "keywords": [
        "exception",
        "error",
        "error code"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Dustin Simon"
    }],
    "autoload": {
        "psr-4": { "Dustin\\Exception\\": "src/" },
        "exclude-from-classmap": ["src/Test/"]
    },
    "require": {
        "php": "^8.1",
        "symfony/http-kernel": "~6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}
